    "description": "Sensor with temperature, humidity, and barometric pressure",
    "contents": [
        {
            "@type": [
                "Telemetry",
                "Temperature"
            ],
            "name": "temperature",
            "displayName": "Temperature",
            "description": "Temperature measured by the ENV sensor.",
