        },
        {
            "@type": [
                "Telemetry",
                "RelativeHumidity"
            ],
            "name": "humidity",
            "displayName": "Humidity",
            "description": "Humidity measured by the ENV sensor.",
            "schema": "double",
            "unit": "percent"
        }
